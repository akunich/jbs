<?xml version="1.0" encoding="utf-8"?>
<!-- @author Alex Keda, for www.host-food.ru -->
<XML>
<!-- *********************************************************************** -->
<Name>Регистратор доменов</Name>
<!-- *********************************************************************** -->
<IsActive>1</IsActive>
<!-- *********************************************************************** -->
<ServiceID>20000</ServiceID>
<!-- *********************************************************************** -->
<Defaults>
	<Address></Address>
	<Login></Login>
	<Monitoring>HTTP=80 HTTPS=443</Monitoring>
	<AdminNotice></AdminNotice>
	<SortID>20000</SortID>
</Defaults>
<!-- *********************************************************************** -->
<!-- *********************************************************************** -->
<SystemID>
	<NullRegistrator>
		<Settings>
			<Name>Ручная регистрация</Name>
			<Address>www.isp.su</Address>
			<Port>80</Port>
			<Protocol>tcp</Protocol>
			<PrefixAPI />
			<PrefixNic />
			<JurName />
			<UseContractRegister>0</UseContractRegister>
			<IsSupportContracts>0</IsSupportContracts>
			<PersonID />
		</Settings>
	</NullRegistrator>
	<!-- ********************************************************************* -->
	<WebNames>
		<Settings>
			<Name>WebNames</Name>
			<Address>www.webnames.ru</Address>
			<Port>81</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic>REGTIME</PrefixNic>
			<JurName>ООО "Регтайм"</JurName>
			<UseContractRegister>0</UseContractRegister>
			<IsSupportContracts>0</IsSupportContracts>
			<PersonID />
		</Settings>
	</WebNames>
	<!-- ********************************************************************* -->
	<NauNet>
		<Settings>
			<Name>NauNet</Name>
			<Address>ssbase.naunet.ru</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic>NAUNET</PrefixNic>
			<JurName>ООО "Наунет СП"</JurName>
			<UseContractRegister>0</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID>Contact login</PersonID>
		</Settings>
	</NauNet>
	<!-- ********************************************************************* -->
	<RuCenter>
		<Settings>
			<Name>RuCenter</Name>
			<Address>www.nic.ru</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic>RU-CENTER</PrefixNic>
			<JurName>ЗАО "Региональный Сетевой Информационный Центр"</JurName>
			<UseContractRegister>1</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID>000000/NIC-D</PersonID>
		</Settings>
	</RuCenter>
	<!-- ********************************************************************* -->
	<MasterName>
		<Settings>
			<Name>MasterName</Name>
			<Address>cp.mastername.ru</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic>REGISTRATOR</PrefixNic>
			<JurName>ООО "Регистратор Доменов"</JurName>
			<UseContractRegister>1</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID />
		</Settings>
	</MasterName>
	<!-- ********************************************************************* -->
	<RegRu>
		<Settings>
			<Name>RegRu</Name>
			<Address>api.reg.ru</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI>/api/regru2</PrefixAPI>
			<PrefixNic>REGRU</PrefixNic>
			<JurName>ООО "Регистратор доменных имён РЕГ.РУ"</JurName>
			<UseContractRegister>0</UseContractRegister>
			<IsSupportContracts>0</IsSupportContracts>
			<PersonID />
		</Settings>
	</RegRu>
	<!-- ********************************************************************* -->
	<LogicBoxes>
		<Settings>
			<Name>LogicBoxes (beta)</Name>
			<Address>www.isp.su</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic />
			<JurName />
			<UseContractRegister>1</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID />
		</Settings>
	</LogicBoxes>
	<!-- ********************************************************************* -->
	<Started>
		<Settings>
			<Name>Started (beta)</Name>
			<Address>api.started.ru</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic />
			<JurName />
			<UseContractRegister>1</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID />
		</Settings>
	</Started>
	<!-- ********************************************************************* -->
	<R01>
		<Settings>
			<Name>R01 (beta)</Name>
			<Address>partner.r01.ru</Address>
			<Port>1443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI>partner_api.khtml</PrefixAPI>
			<PrefixNic>R01</PrefixNic>
			<JurName>ООО "Гарант-Парк-Телеком"</JurName>
			<UseContractRegister>1</UseContractRegister>
			<IsSupportContracts>1</IsSupportContracts>
			<PersonID />
		</Settings>
	</R01>
	<!-- ********************************************************************* -->
	<InternetBS>
		<Settings>
			<Name>InternetBS (beta)</Name>
			<Address>api.internet.bs</Address>
			<Port>443</Port>
			<Protocol>ssl</Protocol>
			<PrefixAPI />
			<PrefixNic />
			<JurName />
			<UseContractRegister>0</UseContractRegister>
			<IsSupportContracts>0</IsSupportContracts>
			<PersonID />
		</Settings>
	</InternetBS>
</SystemID>
<!-- ********************************************************************* -->
<!-- ********************************************************************* -->
<Attribs>
	<!-- *********************************************************************** -->
	<Name>
		<Title>Прочие настройки</Title>
		<Check>Char</Check>
		<Comment>Название</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Отображаемое имя вышестоящего регистратора</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value></Value>
	</Name>
	<!-- *********************************************************************** -->
        <SystemID>
		<Comment>Регистратор</Comment>
		<Check>/^[A-Za-zА-Яа-я0-9\-]{2,25}$/u</Check>
		<Type>Select</Type>
		<Options>
			<NullRegistrator>Ручная регистрация</NullRegistrator>
			<WebNames>WebNames</WebNames>
			<RegRu>RegRu</RegRu>
			<NauNet>NauNet</NauNet>
			<RuCenter>RuCenter</RuCenter>
			<MasterName>MasterName</MasterName>
			<LogicBoxes>LogicBoxes (beta)</LogicBoxes>
			<Started>Started (beta)</Started>
			<R01>R01 (beta)</R01>
			<InternetBS>InternetBS (beta)</InternetBS>
		</Options>
		<Attribs>
			<size>1</size>
			<style>width: 100%;</style>
			<onchange>SettingsUpdate();</onchange>
			<prompt>Вышестоящий регистратор доменов</prompt>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value>RegRu</Value>
	</SystemID>
	<!-- *********************************************************************** -->
	<Comment>
		<Check>Char</Check>
		<Comment>Комментарий</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Надо раскуривать зачем это поле вообще нужно и где юзается</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value></Value>
	</Comment>
	<!-- *********************************************************************** -->
	<PrefixAPI>
		<Check>Char</Check>
		<Comment>Префикс API</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Окончание URL адреса по которому необходимо обращаться к регистратору</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value></Value>
	</PrefixAPI>
	<!-- *********************************************************************** -->
	<ParentID>
		<Check>Char</Check>
		<Comment>Родительский номер</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Только для LogicBoxes</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value></Value>
	</ParentID>
	<!-- *********************************************************************** -->
	<PrefixNic>
		<Title>Параметры трансфера доменов</Title>
		<Check>Char</Check>
		<Comment>Префикс nic-hdl</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Код регистратора, с него начинается поле &quot;registrar&quot; при просмотре информации WhoIs о домене</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value></Value>
	</PrefixNic>
	<!-- *********************************************************************** -->
	<PartnerLogin>
		<Check>Char</Check>
		<Comment>Партнерский аккаунт</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Имя партнёрского аккаунта у регистратора (обычно, совпадает с логином)</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value></Value>
	</PartnerLogin>
	<!-- *********************************************************************** -->
	<PartnerContract>
		<Check>Char</Check>
		<Comment>Партнерский договор</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Данные партнёрского договора - номер, иногда требуется и дата</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value></Value>
	</PartnerContract>
	<!-- *********************************************************************** -->
	<JurName>
		<Check>Char</Check>
		<Comment>Официальное наименование</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Официальное наименование компании регистратора</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value></Value>
	</JurName>
	<!-- *********************************************************************** -->
        <Ns1Name>
		<Title>Сервера имен</Title>
		<Check>DnsDomain</Check>
		<Comment>Первичный</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Сервер DNS на котором будут размещены доменные зоны клиентских сайтов</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value>ns1.isp.su</Value>
	</Ns1Name>
	<!-- *********************************************************************** -->
	<Ns2Name>
		<Check>DnsDomain</Check>
		<Comment>Вторичный</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Сервер DNS на котором будут размещены доменные зоны клиентских сайтов</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>1</IsDuty>
		<Value>ns2.isp.su</Value>
	</Ns2Name>
	<!-- *********************************************************************** -->
	<Ns3Name>
		<Check>DnsDomain</Check>
		<Comment>Дополнительный</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Сервер DNS на котором будут размещены доменные зоны клиентских сайтов</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value />
	</Ns3Name>
	<!-- *********************************************************************** -->
	<Ns4Name>
		<Check>DnsDomain</Check>
		<Comment>Расширенный</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>Сервер DNS на котором будут размещены доменные зоны клиентских сайтов</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value />
	</Ns4Name>
	<!-- *********************************************************************** -->
	<!-- *********************************************************************** -->
	<BalanceLowLimit>
		<Check>/^[0-9]{1,6}$/u</Check>
		<Title>Настройки оповещения о баллансе</Title>
		<Comment>Минимальная сумма</Comment>
		<Type>Input</Type>
		<Attribs>
			<prompt>При снижении балланса счёта у регистратора ниже этой суммы, группе &quot;Бухгалтерия&quot; (при её отсутствии - всем сотрудникам) будут отсылаться уведомления о необходимости пополнения счёта. Для отключения уведомлений, введите ноль</prompt>
			<type>text</type>
			<style>width: 100%;</style>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value>3000</Value>
	</BalanceLowLimit>
	<!-- *********************************************************************** -->
	<!-- *********************************************************************** -->
        <!-- *********************************************************************** -->
	<IsLogging>
		<Title>Скрытые настройки</Title>
		<Check>Char</Check>
		<Comment>Записывать запросы в лог</Comment>
		<CommentAttribs>
			<style>cursor:pointer;</style>
			<onclick>ChangeCheckBox('IsLogging'); return false;</onclick>
			<prompt>Записывать в лог все запросы и ответы сервера</prompt>
		</CommentAttribs>
		<Type>Input</Type>
		<Attribs>
			<type>checkbox</type>
		</Attribs>
		<IsDuty>0</IsDuty>
		<Value>yes</Value>
	</IsLogging>
	<!-- *********************************************************************** -->
	<UseContractRegister>
		<Comment>Регистратор требует договора</Comment>
		<Check>Char</Check>
		<Type>Hidden</Type>
		<IsDuty>0</IsDuty>
		<Value>0</Value>
	</UseContractRegister>
	<!-- *********************************************************************** -->
	<IsSupportContracts>
		<Comment>Регистратор поддерживает договора</Comment>
		<Check>Char</Check>
		<Type>Hidden</Type>
		<IsDuty>0</IsDuty>
		<Value>0</Value>
	</IsSupportContracts>
	<!-- *********************************************************************** -->
	<PersonID>
		<Comment>Идентификатор клиента</Comment>
		<Check>Char</Check>
		<Type>Hidden</Type>
		<IsDuty>0</IsDuty>
		<Value />
	</PersonID>
	<!-- *********************************************************************** -->
</Attribs>
<!-- *********************************************************************** -->
</XML>

