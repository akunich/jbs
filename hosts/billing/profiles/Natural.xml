<?xml version="1.0" encoding="utf-8"?>
<!-- @author Бреславский А.В. (Joonte Ltd.) -->
<XML>
<!-- *********************************************************************** -->
<ProfileName>%Sourname% %Name%</ProfileName>
<!-- *********************************************************************** -->
<Attribs>
 <Sourname>
  <Title>Общая информация</Title>
  <Check>/^[А-ЯЁ]{1}[а-яё\-\w]{1,50}$/u</Check>
  <Comment>Фамилия</Comment>
  <Example>Иванов</Example>
  <Type>Input</Type>
  <Attribs>
   <size>15</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1).toLowerCase());</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </Sourname>
 <!-- ********************************************************************** -->
 <Name>
  <Check>/^[А-ЯЁ]{1}[а-яё\-\s]{1,50}$/u</Check>
  <Comment>Имя</Comment>
  <Example>Петр</Example>
  <Type>Input</Type>
  <Attribs>
   <size>10</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1).toLowerCase());</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </Name>
 <!-- ********************************************************************** -->
 <Lastname>
  <Check>/^[a-zA-Zа-яА-ЯёЁ\s\-]{1,50}$/u</Check>
  <Comment>Отчество</Comment>
  <Example>Викторович</Example>
  <Type>Input</Type>
  <Attribs>
   <size>15</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1));</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </Lastname>
 <!-- ********************************************************************** -->
 <BornDate>
  <Check>/^[0-9]{2}\.[0-9]{2}\.[0-9]{4}$/u</Check>
  <IsDuty>1</IsDuty>
  <Comment>Дата рождения</Comment>
  <Example>18.03.1986</Example>
  <Type>Input</Type>
  <Attribs>
   <size>10</size>
   <onblur>value = value.charAt(0).toUpperCase() + value.substring(1).toLowerCase();</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </BornDate>
 <!-- ********************************************************************** -->
 <Phone>
  <Check>Phone</Check>
  <Comment>Телефон</Comment>
  <Example>+7 495 7558899</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </Phone>
 <!-- ********************************************************************** -->
 <CellPhone>
  <Check>Phone</Check>
  <Comment>Мобильный телефон</Comment>
  <Example>+7 926 7558899</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </CellPhone>
 <!-- ********************************************************************** -->
 <Fax>
  <Check>Phone</Check>
  <Comment>Факс</Comment>
  <Example>+7 095 1551891</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
  </Attribs>
  <IsDuty>0</IsDuty>
  <Value />
 </Fax>
 <!-- ********************************************************************** -->
 <Email>
  <Check>Email</Check>
  <Comment>Электронный адрес</Comment>
  <Example>alexander@yandex.ru</Example>
  <Type>Input</Type>
  <Attribs>
   <size>25</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value>%Email%</Value>
 </Email>
 <!-- ********************************************************************** -->
 <PasportLine>
  <Title>Паспортные данные</Title>
  <Check>/^[0-9a-zA-Zа-яА-ЯёЁ\-]{1,10}$/u</Check>
  <Comment>Серия паспорта</Comment>
  <Example>3422</Example>
  <Type>Input</Type>
  <Attribs>
   <size>5</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </PasportLine>
 <!-- ********************************************************************** -->
 <PasportNum>
  <Check>/^[0-9a-zA-Z]{1,10}$/u</Check>
  <Comment>Номер паспорта</Comment>
  <Example>427789</Example>
  <Type>Input</Type>
  <Attribs>
   <size>15</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </PasportNum>
 <!-- ********************************************************************** -->
 <PasportWhom>
  <Check>/^.{5,250}$/u</Check>
  <Comment>Паспорт выдан</Comment>
  <Example>15 отделением милиции г. Москвы</Example>
  <Type>TextArea</Type>
  <Attribs>
   <rows>3</rows>
   <cols>30</cols>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </PasportWhom>
 <!-- ********************************************************************** -->
 <PasportDate>
  <Check>/^[0-9]{2}\.[0-9]{2}\.[0-9]{4}$/u</Check>
  <Comment>Дата выдачи паспорта</Comment>
  <Example>01.06.2001</Example>
  <Type>Input</Type>
  <Attribs>
   <size>10</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </PasportDate>
 <!-- ********************************************************************** -->
 <pCountry>
  <Title>Почтовый адрес</Title>
  <Check>/^[A-ZА-ЯёЁа-я\-\s]{1,100}$/u</Check>
  <Comment>Страна</Comment>
  <Example>Россия</Example>
  <Type>Select</Type>
  <Options>
   <AZ>Азербайджан</AZ>
   <BY>Беларусь</BY>
   <BG>Болгария</BG>
   <UK>Великобритания</UK>
   <DE>Германия</DE>
   <GE>Грузия</GE>
   <IL>Израиль</IL>
   <KZ>Казахстан</KZ>
   <KG>Киргизия</KG>
   <CA>Канада</CA>
   <CC>Кокосовые острова</CC>
   <LV>Латвия</LV>
   <LT>Литва</LT>
   <MT>Мальта</MT>
   <MD>Молдова</MD>
   <NL>Нидерланды</NL>
   <NO>Норвегия</NO>
   <PL>Польша</PL>
   <RU>Россия</RU>
   <US>США</US>
   <TM>Туркмения</TM>
   <UZ>Узбекистан</UZ>
   <UA>Украина</UA>
   <FI>Финляндия</FI>
   <CZ>Чехия</CZ>
   <EE>Эстония</EE>
  </Options>
  <Attribs>
   <size>1</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value>RU</Value>
 </pCountry>
 <!-- ********************************************************************** -->
 <pState>
  <Check>/^[А-ЯёЁа-я\.\-\s\)\(]{1,60}$/u</Check>
  <Comment>Область / край</Comment>
  <Example>Московская обл.</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1));</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </pState>
 <!-- ********************************************************************** -->
 <pCity>
  <Check>/^[А-ЯёЁа-я\-\s]{1,100}$/u</Check>
  <Comment>Город / населенный пункт</Comment>
  <Example>Москва</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1));</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </pCity>
 <!-- ********************************************************************** -->
 <pDistrict>
  <Check>/^[А-ЯёЁа-я\-\s]{1,100}$/u</Check>
  <Comment>Район</Comment>
  <Example>Ивановский</Example>
  <Type>Input</Type>
  <Attribs>
   <size>20</size>
   <onblur>value = SPrintF('%s%s',value.charAt(0).toUpperCase(),value.substring(1));</onblur>
  </Attribs>
  <IsDuty>0</IsDuty>
  <Value />
 </pDistrict>
 <!-- ********************************************************************** -->
 <pIndex>
  <Check>/^[A-Za-z0-9\s]{3,10}$/u</Check>
  <Comment>Почтовый индекс</Comment>
  <Example>101000</Example>
  <Type>Input</Type>
  <Attribs>
   <size>7</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </pIndex>
 <!-- ********************************************************************** -->
 <pType>
  <Check>/^[A-ZА-ЯёЁа-я\-\s]{1,100}$/u</Check>
  <Comment>Тип адреса</Comment>
  <Example>Улица</Example>
  <Type>Select</Type>
  <Options>
   <Street>улица</Street>
   <Alleyway>переулок</Alleyway>
   <Area>площадь</Area>
   <Thoroughfare>проезд</Thoroughfare>
   <Alley>аллея</Alley>
   <Avenue>проспект</Avenue>
   <Boulevard>бульвар</Boulevard>
   <Wharf>набережная</Wharf>
   <Highway>шоссе</Highway>
   <Pobox>а/я</Pobox>
   <Deadend>тупик</Deadend>
   <Block>квартал</Block>
   <Village>деревня</Village>
   <No>n/a</No>
  </Options>
  <Attribs>
   <size>1</size>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value>Street</Value>
 </pType>
 <!-- ********************************************************************** -->
 <pAddress>
  <Check>/^[^\\]{1,150}$/u</Check>
  <Comment>Адрес</Comment>
  <Example>Ленина, д. 51, кв. 7</Example>
  <Type>TextArea</Type>
  <Attribs>
   <rows>3</rows>
   <cols>30</cols>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </pAddress>
 <!-- ********************************************************************** -->
 <pRecipient>
  <Check>/^[a-zA-Zа-яА-ЯёЁ\s]+$/u</Check>
  <Comment>Получатель</Comment>
  <Example>Иванову Петру Викторовичу</Example>
  <Type>TextArea</Type>
  <Attribs>
   <rows>3</rows>
   <cols>30</cols>
   <onblur>var Matrix = value.split(' ');Result = '';for(var i=0;i&lt;Matrix.length;i++){ var Element = Trim(Matrix[i]);if(Element){ Result += SPrintF('%s%s ',Element.charAt(0).toUpperCase(),Element.substring(1).toLowerCase()); } } value = Trim(Result);</onblur>
  </Attribs>
  <IsDuty>1</IsDuty>
  <Value />
 </pRecipient>
</Attribs>
<!-- *********************************************************************** -->
</XML>
